#include <stdio.h>
#include <iostream>
#include <vector>
#include <set>
using namespace std;

void union_arr(vector<int>ar1 , vector<int>ar2 , int n1, int n2){
    set <int> s; 
    for (int i = 0; i < n1; i++)
    {
        s.insert(ar1[i]);
    }
    for (int i = 0; i < n2; i++)
    {
        s.insert(ar2[i]);
    }
    for (auto it : s)
    {
        cout<<it;
    }
    
    

}

int main() {
    int n1;
    int n2;
    cin>>n1>>n2;
    vector<int>ar1,ar2;
    for (int i = 0; i < n1; i++)
    {
        int x;
        cin >> x;
        ar1.push_back(x);
    }
    for (int i = 0; i < n2; i++)
    {
        int x;
        cin>>x;
        ar2.push_back(x);
    }
    
    union_arr(ar1,ar2,n1,n2);
    return 0;
}
