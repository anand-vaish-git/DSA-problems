import java.util.*;
class duplicate
{
public static void main(String[] maps){
int[] arr= {1,1,2,2,2,3,4,4};
  int high=1;
  int low=0;
  while(high<arr.length){
  if(arr[low]==arr[high]){
    high++;
  }
    else{
    low++;
    arr[low]=arr[high];
    high++;
    }
  }
 int[] newarr= new int[low+1];
 for(int i=0; i<=low; i++){
     newarr[i]=arr[i];
 }
 System.out.print(Arrays.toString(newarr));
    }
}
